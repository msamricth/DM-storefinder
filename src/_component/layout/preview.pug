    {{#ifeq @root.selectedCountryMapping.selectedLanguageIso isoCode}}
    <a href="#" id="country-popup-flyout"    data-url="{{redirectionURL}}" data-component="{{@root.selectedCountryMapping.catalog}}"    data-country="{{@root.selectedCountryMapping.country.isocode}}" >
    <img src="{{@root.selectedCountryMapping.flagUrl}}" alt="{{@root.selectedCountryMapping.country.name}}">
    <span class="country-name">{{@root.selectedCountryMapping.country.name}}</span>
    <span class="country-divider"></span>
    <span class="country-currency">{{currencySign}}{{currencyIso}}</span>
    <i class="fa fa-angle-down" aria-hidden="true"></i>
    </a>
    {{/ifeq}}
    {{/each}}
    <div class="popup-flyout-container" style="display: none;">
    {{#each allCountriesMappings}}
    {{#if selectedCountry}}
    {{else}}
    <a href="#" data-country="{{country.isocode}}" data-component="{{catalog}}"
    {{#each languageSiteMappings}}
    {{#if ../singleLanguage}}
    data-url="{{redirectionURL}}" data-lang="{{isoCode}}"
    {{else}}
    {{#ifeq ../selectedLanguageIso isoCode}}
    data-url="{{redirectionURL}}" data-lang="{{isoCode}}"
    {{/ifeq}}
    {{/if}}
    {{/each}}
    >
    <img data-src="{{flagUrl}}" alt="{{country.name}}">
    <span class="country-name">{{country.name}}</span>
    <span class="country-divider"></span>
    <span class="country-currency"> {{languageSiteMappings.[0].currencySign}}{{languageSiteMappings.[0].currencyIso}} </span>
    </a>
    {{/if}}
    {{/each}}
    </div>
    </div>
    {{#if @root.selectedCountryMapping.singleLanguage}}
